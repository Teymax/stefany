
section.section.service-laser-service-list.nano-service-list.overflow-visible
  .container
    .row.main-block
      .col-12.col-xl-8.d-flex.flex-column.align-items-center.left.custom-py-services-price.man-symbol-bg
        .checkbox-wrapper
          input(type="checkbox" class="checkbox" id="checkboxServiceList")
        .service-list-group.mt-4.d-flex.flex-column.justify-content-center.align-items-center
          - let arrStomachFemale={name: "Живот и спина", services: [{"id":2426299,"title":"Крестцовая область"},{"id":2426300,"title":"Ареола молочных желез"},{"id":2426303,"title":"Белая линия живота"},{"id":2426308,"title":"Верхняя часть спины и плечи"},{"id":2426316,"title":"Живот "},{"id":2426312,"title":"Грудь "},{"id":2426314,"title":"Декольте"},{"id":2426337,"title":"Спина"}]}
          - let arrStomachMale={name: "Живот и спина", services: [{"id":2426299,"title":"Крестцовая область"},{"id":2426300,"title":"Ареола молочных желез"},{"id":2426303,"title":"Белая линия живота"},{"id":2426308,"title":"Верхняя часть спины и плечи"},{"id":2426317,"title":"Живот "},{"id":2426313,"title":"Грудь "},{"id":2426315,"title":"Декольте "},{"id":2426337,"title":"Спина"}]}
          - let arrLegsFemale={name: "Область ног", services: [{"id":2426301,"title":"Бедра"},{"id":2426302,"title":"Бедра частично (наружная)"},{"id":2426310,"title":"Голени"},{"id":2426311,"title":"Голени с коленями"},{"id":2426321,"title":"Колени"},{"id":2426325,"title":"Ноги "},{"id":2426327,"title":"Пальцы ног"}]}
          - let arrLegsMale={name: "Область ног", services: [{"id":2426301,"title":"Бедра"},{"id":2426302,"title":"Бедра частично (наружная"},{"id":2426310,"title":"Голени"},{"id":2426311,"title":"Голени с коленями"},{"id":2426321,"title":"Колени"},{"id":2426327,"title":"Пальцы ног"}]}
          - let arrBikiniFemale = {name: "Бикини", services: [{"id":2426304,"title":"Бикини глубокое"},{"id":2426306,"title":"Бикини не глубокое (бикини по линии трусиков)"},{"id":2426324,"title":"Межьягодичная складка"},{"id":2426341,"title":"Ягодицы"}]}
          - let arrBikiniMale = {name: "Бикини", services: [{"id":2426305,"title":"Бикини глубокое"},{"id":2426306,"title":"Бикини не глубокое (бикини по линии трусиков)"},{"id":2426318,"title":"Зона лобка для "},{"id":2426324,"title":"Межьягодичная складка"},{"id":2426341,"title":"Ягодицы"}]}
          - let arrArmsFemale ={name: "Область рук", services: [{"id":2426335,"title":"Руки полностью"},{"id":2426319,"title":"Кисти рук + пальцы "},{"id":2426331,"title":"Подмышки "},{"id":2426334,"title":"Предплечья "},{"id":2426328,"title":"Плечи"}]}
          - let arrArmsMale = {name: "Область рук", services: [{"id":2426336,"title":"Руки полностью "},{"id":2426320,"title":"Кисти рук + пальцы "},{"id":2426332,"title":"Подмышки "},{"id":2426333,"title":"Предплечья "},{"id":2426328,"title":"Плечи"}]}
          - let arrFaceFemale = {name: "Область лица", services: [{"id":2426298,"title":"Все лицо"},{"id":2426296,"title":"Верхняя губа "},{"id":2426307,"title":"Брови"},{"id":2426309,"title":"Виски"},{"id":2426322,"title":"Линия лба"},{"id":2426323,"title":"Межбровье"},{"id":2426329,"title":"Подбородок "},{"id":2426338,"title":"Уши"},{"id":2426339,"title":"Шея (задняя или передняя часть)"},{"id":2426340,"title":"Щеки"}]}
          - let arrFaceMale = {name: "Область лица", services: [{"id":2426298,"title":"Все лицо"},{"id":2426297,"title":"Верхняя губа "},{"id":2426307,"title":"Брови"},{"id":2426309,"title":"Виски"},{"id":2426322,"title":"Линия лба"},{"id":2426323,"title":"Межбровье"},{"id":2426330,"title":"Подбородок "},{"id":2426339,"title":"Шея (задняя или передняя часть)"},{"id":2426340,"title":"Щеки"}]}
          - let allCategoryFemale = [arrFaceFemale,  arrArmsFemale, arrBikiniFemale, arrLegsFemale, arrStomachFemale]
          - let allCategoryMale = [arrFaceMale,  arrArmsMale, arrBikiniMale, arrLegsMale, arrStomachMale]

          .list-female
            each category in allCategoryFemale
              h2.h2-title.text-color-dark.service-section-header-mb.mt-3.text-center #{category.name}
              .checkbox-group.pb-4
                each service in category.services
                  .checkbox-row.d-flex.align-items-start.justify-content-between.py-2(id=service.id data-sex='0')
                    .column-right.d-flex.align-items-start
                      +defaultCheckbox('dark')
                      p.paragraph-text.text-color-lightdark.ml-3.mb-0 #{service.title}
                    .column-left
                      p.paragraph-text.text-color-lightdark.mr-3.mb-0.item-time
                      p.paragraph-text.text-color-lightdark.mb-0.font-weight-bold.item-price
          .list-male
            each category in allCategoryMale
              h2.h2-title.text-color-dark.service-section-header-mb.mt-3.text-center #{category.name}
              .checkbox-group.pb-4
                each service in category.services
                  .checkbox-row.d-flex.align-items-start.justify-content-between.py-2(id=service.id data-sex='1')
                    .column-right.d-flex.align-items-start
                      +defaultCheckbox('dark')
                      p.paragraph-text.text-color-lightdark.ml-3.mb-0 #{service.title}
                    .column-left
                      p.paragraph-text.text-color-lightdark.mr-3.mb-0.item-time
                      p.paragraph-text.text-color-lightdark.mb-0.font-weight-bold.item-price


      .col-12.col-xl-4.right.custom-py-services-price.d-none.d-xl-block
        .stash.d-flex.flex-column.align-items-center.pb-5
          .sale-container.d-flex.flex-column.align-items-center
            h4.sale-title.text-color-white Вы выбрали такие зоны:

          #choosenServices.checkbox-group.d-flex.flex-column.align-items-center.mt-4.pb-4

          //.sale-group.d-flex.justify-content-between
            p.paragraph-text.text-w-light.text-color-white Акция
            p.paragraph-text.text-w-bold.text-color-white 40%
          .price-group.d-flex.justify-content-between
          .price-count
            .price-block.d-flex.justify-content-start
              img(src="assets/img/laser-page/time.svg")
              span.paragraph-text.text-w-bold.text-color-white.ml-2.serviceListTime 0 мин
            .price-block.d-flex.justify-content-end
              img(src="assets/img/laser-page/price.svg")
              span.paragraph-text.text-w-bold.text-color-white.ml-2.serviceListSum 0 грн
          .c-btn.d-flex.justify-content-between.w-332
            .button.btn-first.btn-simple.svg.caset(data-toggle='modal', data-target='#modalServiceListPay')
              .text-section
                +casetSvg
                span оплатить
            .button.btn-second.btn-simple.svg.pen(data-toggle='modal', data-target='#modalServiceListSinugUp')
              .text-section
                +penSvg
                span записаться


