section.section.service-laser-results.pt-0.pb-5
    .container
        h1.h2-title.text-color-dark.text-center.my-5 Вас также может заинтересовать
        .container.slider-wrap
            .row.no-gutters
                .h1-title.h-center.white-text Дипломы и сертификаты
            .row.no-gutters
                .diploma-slider.owl-carousel.slider-block
                    - let arrResult = [{name:"Лазерная эпиляция / ДО", img:"result1"},{name:"Лазерная эпиляция / после",img:"result2"},{name:"Маска для кожи вокруг глаз / ДО",img:"result3"},{name:"Маска для кожи вокруг глаз / после",img:"result4"},{name:"Лазерная эпиляция / ДО", img:"result1"},{name:"Лазерная эпиляция / после",img:"result2"},{name:"Маска для кожи вокруг глаз / ДО",img:"result3"},{name:"Маска для кожи вокруг глаз / после",img:"result4"},{name:"Лазерная эпиляция / ДО", img:"result1"},{name:"Лазерная эпиляция / после",img:"result2"},{name:"Маска для кожи вокруг глаз / ДО",img:"result3"},{name:"Маска для кожи вокруг глаз / после",img:"result4"},{name:"Лазерная эпиляция / ДО", img:"result1"},{name:"Лазерная эпиляция / после",img:"result2"},{name:"Маска для кожи вокруг глаз / ДО",img:"result3"},{name:"Маска для кожи вокруг глаз / после",img:"result4"}];
                        each element in arrResult
                            - let resultPath = "./assets/img/service-laser-page/" + element.img + ".png"
                                a.diploma-item(href=resultPath, data-fancybox="" )
                                    img.w-100(src=resultPath, alt="result-image")
                                    .slider-item-footer.d-flex.flex-column.align-items-center.justify-content-center
                                        p.paragraph-text.text-color-lightdark.mb-0 #{element.name}
