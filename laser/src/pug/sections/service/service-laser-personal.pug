section.section.service-laser-personal
  .container.slider-wrap
    h2.h2-title.text-center Наши мастера
    .row.no-gutters
      .specialists-slider.owl-carousel.slider-block.slider-pad
        - let arrPersonal = [{name:"Ксения Станиславская", img:"slider2"},{name:"Демчук Виктория",img:"slider1"},{name:"Ирина Гутель",img:"slider3"},{name:"Андриевич Ульяна",img:"slider4"},{name:"Ксения Станиславская", img:"slider2"},{name:"Демчук Виктория",img:"slider1"},{name:"Ирина Гутель",img:"slider3"},{name:"Андриевич Ульяна",img:"slider4"}];
            each element in arrPersonal
                -let path = "./assets/img/service-laser-page/" + element.img + ".png"
                    .specialists-item.d-flex.flex-column.align-items-center
                        .specialists-item-container.mb-3.w-100(href = path, data-fancybox="" )
                            .specialists-img.w-100
                                img.w-100(src = path, alt="personal-image")
                            .specialists-info.d-flex.flex-column.align-items-start.w-100.p-3
                                p.paragraph-text.text-color-dark По образованию медсестра и реабилитолог
                                p.paragraph-text.text-color-dark.text-w-bold.mt-3 Работает уже 9 лет
                                p.paragraph-text.text-color-dark.mt-3 Сделает массаж, SPA-услуги, лазерную эпиляцию, LPG-массаж
                        p.h3-title #{element.name}

